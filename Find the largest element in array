import java.util.*;
public class arrayLargestEle {
    public static void main(String[] args){
        int arr[] = {1,2,6,3,5};
        largestEle(arr);
    }
    public static void largestEle(int arr[]){
        //int max=arr[0];
        int max = Integer.MIN_VALUE;
        for(int i=0;i<arr.length;i++){
            if(arr[i]>max){
                max=arr[i];
            }
        }
        System.out.println(max);
    }
}
